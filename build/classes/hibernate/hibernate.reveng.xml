<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="student" match-catalog="hibernate"></table-filter>
	<table-filter match-name="employee" match-catalog="hibernate"></table-filter>
	<table catalog="hibernate" name="student"></table>
	<table catalog="hibernate" name="employee"></table>
</hibernate-reverse-engineering>